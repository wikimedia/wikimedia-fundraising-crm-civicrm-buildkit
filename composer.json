    "civicrm/composer-downloads-plugin": "^3.0",
    "squizlabs/php_codesniffer": ">=2.7 <4.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/civicrm/coder.git"
    },
    {
      "type": "git",
      "url": "https://github.com/totten/paratest.git"
    }
  ],
  "extra": {
    "downloads": {
      "amp": {"version": "0.6.1", "url": "https://download.civicrm.org/amp/amp.phar-{$version}", "path": "bin/amp", "type": "phar"},
